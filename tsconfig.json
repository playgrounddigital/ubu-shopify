{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["dom", "dom.iterable", "esnext"],
    "baseUrl": ".",
    "paths": {
      "~/data/*": ["data/*"],
      "~/app/*": ["app/*"],
      "~/graphql/*": ["graphql/*"],
      "~/components/*": ["components/*"],
      "~/context/*": ["context/*"],
      "~/styles/*": ["styles/*"],
      "~/helpers/*": ["helpers/*"],
      "~/pages/*": ["pages/*"],
      "~/lib/*": ["lib/*"],
      "~/constants/*": ["constants/*"],
      "~/types/*": ["types/*"],
      "~/config/*": ["config/*"],
      "~/hooks/*": ["hooks/*"],
      "~/public/*": ["public/*"],
      "~/icons/*": ["public/img/icons/*"],
      "~/logos/*": ["public/img/logos/*"],
      "react": ["./node_modules/@types/react"]
    },
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "dist/types/**/*.ts"],
  "exclude": ["node_modules", ".next", "dist"]
}
